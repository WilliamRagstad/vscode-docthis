{
  "name": "wr-docthis",
  "displayName": "William's Document This",
  "description": "Automatically generates detailed JSDoc comments in TypeScript and JavaScript files.",
  "version": "0.9.6",
  "publisher": "WilliamRagstad",
  "icon": "images/icon.png",
  "galleryBanner": {
    "color": "#1e1e2f",
    "theme": "dark"
  },
  "author": {
